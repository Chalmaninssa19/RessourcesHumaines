<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>AddBesoinServlet</servlet-name>
        <servlet-class>servlet.besoin.AddBesoinServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddTaskServlet</servlet-name>
        <servlet-class>servlet.besoin.AddTaskServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProfileValidedServlet</servlet-name>
        <servlet-class>servlet.besoin.ProfileValidedServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddWorkLoad</servlet-name>
        <servlet-class>servlet.besoin.AddWorkLoad</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddWorkLoadServlet</servlet-name>
        <servlet-class>servlet.besoin.AddWorkLoadServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddBesoinServlet</servlet-name>
        <url-pattern>/addBesoinServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddTaskServlet</servlet-name>
        <url-pattern>/addTaskServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProfileValidedServlet</servlet-name>
        <url-pattern>/profileValidedServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddWorkLoad</servlet-name>
        <url-pattern>/addWorkLoad</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name
        >AddWorkLoadServlet</servlet-name>
        <url-pattern>/addWorkLoadServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
